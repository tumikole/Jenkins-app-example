pipeline {
     agent any
     stages {
        stage("Build") {
            steps {
                                sh "git clone https://github.com/tumikole/Jenkins-app-example.git"

                sh "cd /home/thecodingground/workspace/Jenkins-app-example/"
            }
        }
        stage("Test") {
            steps {
                sh "sudo npm install"
                sh "sudo npm run build"
            }
        }
        stage("Deploy") {
            steps {
                sh "sudo "
                // sh "sudo cp -r ${WORKSPACE}/build/ /var/www/jenkins-react-app/"
            }
        }
    }
}